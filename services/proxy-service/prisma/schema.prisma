// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model RequestLog {
  id          String   @id @default(uuid())
  provider    String
  endpoint    String
  method      String
  statusCode  Int
  responseTime Int
  tokensUsed  Int?
  cost        Float?
  userId      String?
  createdAt   DateTime @default(now())

  @@map("request_logs")
}
