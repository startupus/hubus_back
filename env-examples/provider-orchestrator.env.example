# Provider Orchestrator Environment Variables

# Service Port
PORT=3002

# Database
DATABASE_URL=postgresql://postgres:password@localhost:5434/orchestrator_db?schema=public

# RabbitMQ
RABBITMQ_URL=amqp://guest:guest@localhost:5672

# Service URLs
BILLING_SERVICE_URL=http://billing-service:3004
PROXY_SERVICE_URL=http://proxy-service:3003

# Provider Configuration
DEFAULT_PROVIDER=openrouter
FALLBACK_PROVIDER=openai

# Load Balancing
LOAD_BALANCE_STRATEGY=round_robin

# Logging
LOG_LEVEL=info
