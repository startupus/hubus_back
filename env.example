# Database URLs
AUTH_DATABASE_URL="postgresql://postgres:password@localhost:5432/auth_db?schema=public"
BILLING_DATABASE_URL="postgresql://postgres:password@localhost:5433/billing_db?schema=public"
ORCHESTRATOR_DATABASE_URL="postgresql://postgres:password@localhost:5434/orchestrator_db?schema=public"

# Redis
REDIS_URL="redis://localhost:6379"

# RabbitMQ
RABBITMQ_URL="amqp://guest:guest@localhost:5672"

# JWT
JWT_SECRET="your_secret_key"
JWT_EXPIRES_IN="24h"

# API Keys - ЗАМЕНИТЕ НА ВАШИ РЕАЛЬНЫЕ КЛЮЧИ

# OpenAI API Key (формат: sk-proj-...)
OPENAI_API_KEY="sk-proj-your-openai-api-key-here"

# OpenRouter API Key (формат: sk-or-v1-...)
OPENROUTER_API_KEY="sk-or-v1-your-openrouter-api-key-here"

# YandexGPT API Key (формат: AQVN...)
# Получить можно в консоли Yandex Cloud: https://console.cloud.yandex.ru/
YANDEX_API_KEY="AQVN-your-yandex-api-key-here"

# YandexGPT Folder ID (формат: b1g...)
# Получить можно в консоли Yandex Cloud в разделе "Облако"
YANDEX_FOLDER_ID="b1g-your-yandex-folder-id-here"

# Service Ports
API_GATEWAY_PORT=3000
AUTH_SERVICE_PORT=3001
PROVIDER_ORCHESTRATOR_PORT=3002
PROXY_SERVICE_PORT=3003
BILLING_SERVICE_PORT=3004
ANALYTICS_SERVICE_PORT=3005
CLASSIFICATION_SERVICE_PORT=3006
CERTIFICATION_SERVICE_PORT=3007
SAFETY_SERVICE_PORT=3008

# Service URLs for API Gateway
AUTH_SERVICE_URL=http://auth-service:3001
BILLING_SERVICE_URL=http://billing-service:3004
ANALYTICS_SERVICE_URL=http://analytics-service:3005
PROXY_SERVICE_URL=http://proxy-service:3003
CLASSIFICATION_SERVICE_URL=http://classification-service:3006
CERTIFICATION_SERVICE_URL=http://certification-service:3007
SAFETY_SERVICE_URL=http://safety-service:3008

# gRPC Ports
AUTH_GRPC_PORT=50051
BILLING_GRPC_PORT=50052
ANALYTICS_GRPC_PORT=50053
