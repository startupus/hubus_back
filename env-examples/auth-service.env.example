# Auth Service Environment Variables

# Service Port
PORT=3001

# Database
DATABASE_URL=postgresql://postgres:password@localhost:5432/auth_db?schema=public

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=24h

# Password Hashing
BCRYPT_ROUNDS=12

# API Key Configuration
API_KEY_LENGTH=32
API_KEY_PREFIX=hub_

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Logging
LOG_LEVEL=info
