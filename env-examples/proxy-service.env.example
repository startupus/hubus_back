# Proxy Service Environment Variables

# Service Port
PORT=3003

# RabbitMQ
RABBITMQ_URL=amqp://guest:guest@localhost:5672

# External API Keys (ЗАМЕНИТЕ НА ВАШИ РЕАЛЬНЫЕ КЛЮЧИ)
OPENAI_API_KEY=sk-proj-your-openai-api-key-here
OPENROUTER_API_KEY=sk-or-v1-your-openrouter-api-key-here
YANDEX_API_KEY=AQVN-your-yandex-api-key-here
YANDEX_FOLDER_ID=b1g-your-yandex-folder-id-here

# Service URLs
BILLING_SERVICE_URL=http://billing-service:3004
ANALYTICS_SERVICE_URL=http://analytics-service:3005

# Request Configuration
REQUEST_TIMEOUT=30000
MAX_RETRIES=3
RETRY_DELAY=1000

# Logging
LOG_LEVEL=info
